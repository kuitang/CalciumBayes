	Number = {10},
	Pages = {e24899},
	Pmc = {PMC3198725},
	Pmid = {22031814},
	Pst = {ppublish},
	Title = {Automated detection and segmentation of synaptic contacts in nearly isotropic serial electron microscopy images},
	Volume = {6},
