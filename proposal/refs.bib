	Issue = {1},
	Journal = {Journal of Computational Neuroscience},
	Keyword = {Biomedical and Life Sciences},
	Note = {10.1007/s10827-009-0179-x},
	Pages = {107-126},
	Publisher = {Springer Netherlands},
	Title = {A new look at state-space models for neural data},
